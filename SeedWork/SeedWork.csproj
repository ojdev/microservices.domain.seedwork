<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>YYY.Microservices.Domain.SeedWork</AssemblyName>
    <RootNamespace>YYY.Microservices.Domain.SeedWork</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.0.4</Version>
    <Authors>欧俊</Authors>
    <PackageProjectUrl>https://github.com/ojdev/yyy.microservices.domain</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ojdev/yyy.microservices.domain</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageId>YYY.Microservices.Domain.SeedWork</PackageId>
    <Product>YYY.Microservices.Domain.SeedWork</Product>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="MediatR" Version="5.0.1" />
  </ItemGroup>

</Project>
